{
  "name": "mevn-orm",
  "version": "2.1.1",
  "description": "simple ORM for express js",
  "type": "commonjs",
  "scripts": {
    "prepare": "tsc",
    "pretest": "NODE_ENV=development node initDb && tsc",
    "test": "NODE_ENV=testing mocha  --exit",
    "init:db": "NODE_ENV=development node initDb",
    "build": "tsc",
    "migrate": "knex migrate:latest",
    "watch": "tsc --watch"
  },
  "peerDependencies": {
    "knex": "^0.21"
  },
  "keywords": [
    "ORM",
    "express",
    "Mysql",
    "mevn"
  ],
  "author": "Stanley Masinde",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "import-cwd": "^3.0.0",
    "knex": "^0.21.15",
    "pluralize": "^8.0.0",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^14.14.19",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3"
  },
  "directories": {
    "test": "test"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StanleyMasinde/mevn-orm.git"
  },
  "bugs": {
    "url": "https://github.com/StanleyMasinde/mevn-orm/issues"
  },
  "homepage": "https://github.com/StanleyMasinde/mevn-orm#readme",
  "main": "dist/index.js"
}
