{
  "name": "mevn-orm",
  "version": "4.0.2",
  "description": "simple ORM for express js",
  "type": "module",
  "scripts": {
    "pretest": "node --import tsx initDb.ts",
    "test": "vitest run",
    "init:db": "node --import tsx initDb.ts",
    "typecheck": "tsc --noEmit",
    "migrate": "node --import tsx scripts/migrate.ts latest",
    "migrate:make": "node --import tsx scripts/migrate.ts make",
    "migrate:rollback": "node --import tsx scripts/migrate.ts rollback",
    "migrate:list": "node --import tsx scripts/migrate.ts list",
    "migrate:version": "node --import tsx scripts/migrate.ts version",
    "lint": "eslint --ext .js,.ts ./",
    "build": "tsc -p tsconfig.build.json",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "knex": "^3.0.0",
    "mysql2": "^3.9.3"
  },
  "peerDependenciesMeta": {
    "knex": {
      "optional": false
    }
  },
  "keywords": [
    "ORM",
    "express",
    "Mysql",
    "mevn",
    "database",
    "orm",
    "orm-express",
    "orm-mysql"
  ],
  "author": "Stanley Masinde <stanmasinde@gmail.com> (https://stanleymasinde.github.io)",
  "contributors": [
    "Stanley Masinde <stanmasinde@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.24.3",
    "@babel/eslint-plugin": "^7.24.0",
    "@types/node": "^24.6.0",
    "@faker-js/faker": "^10.0.0",
    "dotenv": "^17.0.1",
    "knex": "^3.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "mysql": "^2.18.1",
    "sqlite3": "^5.1.7"
  },
  "directories": {
    "src": "src",
    "test": "test"
  },
  "packageManager": "pnpm@10.29.2",
  "private": false,
  "repository": "git+https://github.com/StanleyMasinde/mevn-orm.git",
  "bugs": {
    "url": "https://github.com/StanleyMasinde/mevn-orm/issues",
    "email": "stanleymasinde1@gmail.com"
  },
  "homepage": "https://github.com/StanleyMasinde/mevn-orm#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ]
}
